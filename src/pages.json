{
  "easycom": {
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
  },
  "pages": [
    {
      "path": "pages/index/index",
      "enablePullDownRefresh": true
    },
 
    
    {
      "path": "pages/health/health"
    },
    {
      "path": "pages/all-goods/all-goods",
      "style": {
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/cart/cart",
      "enablePullDownRefresh": true
    },
    {
      "path": "pages/my/myPage"
    },
    {
      "path": "pages/login/login"
    },
    {
      "path": "pages/login/protocol/index"
    },
    {
      "path": "pages/login/updatePhone/updatePhone"
    },
    {
      "path": "pages/login/phone/phone"
    }
  ],
  "subPackages": [
    {
      "root": "pagesA",
      "pages": [
        {
          "path": "out/out"
        }
      ]
    }
  ],
  "preloadRule": {
    "pagesA/out/out": {
      "network": "all",
      "packages": [
        "__APP__"
      ]
    }
  },
  "globalStyle": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "xx小程序",
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom",
    "backgroundColor": "#F8F8F8",
    "usingComponents": {
      "navBar": "/components/navBar/navBar"
    }
  },
  "tabBar": {
    "backgroundColor": "#ffffff",
    "color": "#7F7F7F",
    "selectedColor": "#000000",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "/static/images/tab/home.png",
        "selectedIconPath": "/static/images/tab/selectHome.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/all-goods/all-goods",
        "iconPath": "/static/images/tab/mall.png",
        "selectedIconPath": "/static/images/tab/selectMall.png",
        "text": "品牌"
      },
      {
        "pagePath": "pages/cart/cart",
        "iconPath": "/static/images/tab/car.png",
        "selectedIconPath": "/static/images/tab/selectCar.png",
        "text": "购物车"
      },
      {
        "pagePath": "pages/my/myPage",
        "iconPath": "/static/images/tab/my.png",
        "selectedIconPath": "/static/images/tab/selectUser.png",
        "text": "我的"
      }
    ]
  }
}